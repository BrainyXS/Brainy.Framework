<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Brainy.Framework.Testtool.TestPage2.OtherView">
    <StackPanel>
        <TextBlock Text="This is Page 2" Margin="15"/>
        <Button Content="Go back again" Margin="15" Command="{Binding GoBackCommand}" />
        <TextBlock Text="{Binding TestText}" Margin="15"/>

    </StackPanel>
</UserControl>
